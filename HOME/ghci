:set +m
:set prompt "λ➔ "
:set stop :list
:def sl const $ return ":steplocal"
:def hoogle \str -> return $ ":! hoogle --count=15 \"" ++ str ++ "\""
:def doc \q -> return $ ":! hoogle --info \"" ++ q ++ "\""

